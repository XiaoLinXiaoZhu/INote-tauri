{
  "name": "i-notes",
  "version": "0.6.1",
  "private": true,
  "author": "heiyehk",
  "description": "I便笺拥有漂亮的过度效果，允许开启多个窗口方便在桌面端更方便的记录文字",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/heiyehk/electron-vue3-inote"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/heiyehk/electron-vue3-inote/issues",
    "email": "heiyehk@foxmail.com"
  },
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "vue-tsc && bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "bun run log && git add . && cz"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.6.0",
    "@tauri-apps/plugin-dialog": "^2.3.0",
    "@tauri-apps/plugin-fs": "^2.4.0",
    "@tauri-apps/plugin-sql": "^2.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7",
    "vditor": "^3.10.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.6.0",
    "@types/crypto-js": "^4.2.2",
    "@types/bun": "^1.2.11",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.1",
    "less": "^4.2.0",
    "prettier": "^3.2.5",
    "typescript": "^5.7.3",
    "vite": "^7.0.0",
    "vue-tsc": "^3.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "packageManager": "pnpm@9.13.2+sha512.88c9c3864450350e65a33587ab801acf946d7c814ed1134da4a924f6df5a2120fd36b46aab68f7cd1d413149112d53c7db3a4136624cfd00ff1846a0c6cef48a"
}
